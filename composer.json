{
  "name": "enpii/enpii-base",
  "description": "The Base plugin for Theme and Plugin development. It requires ACF pro to work.",
  "type": "wordpress-muplugin",
  "authors": [
    {
      "name": "Trac Nguyen",
      "email": "npbtrac@yahoo.com"
    },
    {
      "name": "Enpii",
      "email": "contact@enpii.com"
    }
  ],
  "require": {
    "laravel/framework": "~7.2.2",
    "laravel/socialite": "~4.3.2",
    "laravelcollective/html": "~6.1.0",
    "php": ">=7.2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.5.1",
    "mockery/mockery": "~1.3.1",
    "symfony/var-dumper": "~5.0.5",
    "ezyang/htmlpurifier": "~4.6",
    "codeception/codeception": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Enpii\\Wp\\EnpiiBase\\": "src",
      "Enpii\\Wp\\EnpiiBase\\App\\": "src/app",
      "Enpii\\Wp\\EnpiiBase\\Libs\\": "src/libs",
      "Enpii\\Wp\\EnpiiBase\\Libs\\Traits\\": "src/libs/traits",
      "Enpii\\Wp\\EnpiiBase\\Helpers\\": "src/helpers",
      "Enpii\\Wp\\EnpiiBase\\Helpers\\Interfaces\\": "src/helpers/interfaces",
      "Enpii\\Wp\\EnpiiBase\\Helpers\\Traits\\": "src/helpers/traits",
      "Enpii\\Wp\\EnpiiBase\\Services\\": "src/services",
      "Enpii\\Wp\\EnpiiBase\\Base\\": "src/base",
      "Enpii\\Wp\\EnpiiBase\\Helper\\": "src/helper",
      "Enpii\\Wp\\EnpiiBase\\Component\\": "src/component"
    }
  }
}
