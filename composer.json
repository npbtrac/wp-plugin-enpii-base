{
  "name": "enpii/enpii-base",
  "description": "The Base plugin for Theme and Plugin development.",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Trac Nguyen",
      "email": "npbtrac@yahoo.com"
    },
    {
      "name": "Enpii",
      "email": "contact@enpii.com"
    }
  ],
  "require": {
    "laravel/framework": "~7.2.2",
    "laravelcollective/html": "~6.1.0",
	"ezyang/htmlpurifier": "~4.6",
    "php": ">=7.2.0"
  },
  "require-dev": {
	"inpsyde/php-coding-standards": "^0.13",
	"johnpbloch/wordpress-core": "^5.3",
	"codeception/codeception": "^4.1",
	"codeception/module-asserts": "^1.0.0",
	"codeception/aspect-mock": "*",
	"mockery/mockery": "~1.3.1",
	"brain/monkey": "^2.2",
	"symfony/var-dumper": "^5.1"
  },
  "replace": {
	"phpunit/phunit": "~8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Enpiicom\\WpPlugin\\EnpiiBase\\": "src",
      "Enpiicom\\WpPlugin\\EnpiiBase\\App\\": "src/app",
      "Enpiicom\\WpPlugin\\EnpiiBase\\Libs\\": "src/libs"
    }
  },
  "autoload-dev": {
	"psr-4": {
	  "Enpiicom\\WpPlugin\\EnpiiBase\\Tests\\": "tests"
	}
  }
}
